        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Dao.Mapper.ServiceMapper">

    <resultMap id="ServiceResultMap" type="Pojo.Service">
        <result column="service_id" property="sid"></result>
        <result column="service_name" property="serviceName"></result>
